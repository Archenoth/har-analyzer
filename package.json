{
  "name": "har-analyzer",
	"displayName": "HAR Analyzer",
	"description": "Open and analyze HAR files natively",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.67.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
        "onCommand:har-analyzer.helloWorld",
        "onCommand:har-analyzer.analyze"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{"title": "Hello World", "command": "har-analyzer.helloWorld"},
			{"title": "Analyze", "command": "har-analyzer.analyze"}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
        "@types/vscode": "^1.67.0",
        "@types/glob": "^7.2.0",
        "@types/mocha": "^9.1.1",
        "@types/node": "14.x",
        "eslint": "^8.14.0",
        "glob": "^8.0.1",
        "mocha": "^9.2.2",
        "typescript": "^4.6.4",
        "@vscode/test-electron": "^2.1.3"
    }

}
